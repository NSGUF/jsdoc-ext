{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "recurse": true,
    "template": "template/tui.jsdoc-template",
    "encoding": "utf8",
    "destination": "./docs/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "staticFiles": {
        "include": ["./template/tui.jsdoc-template/static"]
      }
    }
  }
}
